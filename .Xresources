! Termbar
termbar*geometry:	136x1+0+0
termbar*faceName:	Monospace-10
termbar*internalBorder:	3
termbar*saveLines:	0
termbar*scrollBar:	false
termbar*title:		termbar
termbar*background:	#242424
termbar*foreground:	#eeeeee

!! Terminal (ST) Theming

!! Transparency (0-1):
st.alpha: 0.92

!! Set a default font and font size as below:
st.font: DejaVu Sans Mono:pixelsize=14;

! st.termname: st-256color
! st.borderpx: 2

!! Set the background, foreground and cursor colors as below:
!! st.background: #161616
